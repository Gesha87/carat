{
    "name": "gesha87/acra",
    "description": "Acra server for Android Crash Reports",
    "keywords": ["acra", "yii2"],
    "homepage": "https://github.com/gesha87/carat",
    "type": "project",
    "support": {
        "source": "https://github.com/gesha87/carat"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
		"yiisoft/yii2-debug": "*",
        "yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-mongodb": "*",
		"miloschuman/yii2-highcharts-widget": "*",
		"cakebake/yii2-bootstrap-select": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
